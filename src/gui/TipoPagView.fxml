<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="414.0" prefWidth="696.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.TipoPagController">
   <children>
      <Label contentDisplay="CENTER" text="Cadastro de Tipos de Pagamento" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" left="100.0" top="5.0" />
         </VBox.margin>
         <padding>
            <Insets left="50.0" />
         </padding></Label>
      <HBox prefHeight="404.0" prefWidth="740.0">
         <children>
            <TableView fx:id="tableViewTipoPagamento" prefHeight="177.0" prefWidth="527.0">
              <columns>
                <TableColumn fx:id="tableColumnExcluir" prefWidth="45.0" />
                  <TableColumn fx:id="tableColumnEditar" prefWidth="51.0" />
                  <TableColumn fx:id="tableColumnID" prefWidth="36.0" />
                  <TableColumn fx:id="tableColumnNome" maxWidth="1.7976931348623157E308" prefWidth="287.0" />
              </columns>
               <padding>
                  <Insets bottom="1.0" />
               </padding>
            </TableView>
            <Pane prefHeight="100.0" prefWidth="426.0">
               <children>
                  <GridPane prefHeight="75.0" prefWidth="351.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="212.0" minWidth="10.0" prefWidth="43.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="212.0" minWidth="10.0" prefWidth="40.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="378.0" minWidth="10.0" prefWidth="120.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="378.0" minWidth="10.0" prefWidth="161.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <TextField fx:id="txtId" editable="false" promptText="ID" GridPane.columnIndex="1" />
                        <TextField fx:id="txtNome" prefHeight="25.0" prefWidth="221.0" promptText="NOVO" GridPane.columnIndex="2" />
                        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
                           <children>
                              <Button fx:id="btSalvar" mnemonicParsing="false" onAction="#onBtSalvar" text="Salvar">
                                 <HBox.margin>
                                    <Insets right="5.0" />
                                 </HBox.margin></Button>
                              <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#onBtCancelar" text="Cancelar" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" />
                           </padding>
                           <GridPane.margin>
                              <Insets top="20.0" />
                           </GridPane.margin>
                        </HBox>
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </GridPane>
               </children>
            </Pane>
         </children>
      </HBox>
   </children>
</VBox>
